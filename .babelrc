{
  "presets": [
    ["env", { "modules": false }],
    "es2015",
    "stage-3"
  ],
  "plugins": 
    ["transform-runtime", 
    ["component", 
      [{
        "libraryName": "element-ui",
        "styleLibraryName": "theme-default"
      }]
    ]
  ],
  "comments": false,
  "env": {
    "test": {
      "presets": ["env", "stage-3"],
      "plugins": [ "istanbul" ]
    }
  }
}
